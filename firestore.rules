rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /config/{configFile} {
        allow write: if request.auth.uid != null;
        allow read: if true;
    }
    match /users/{userId} {
        allow write, read: if request.auth.uid != null;
    }
    match /chatsv2/{chatId} {
        allow write, read: if request.auth.uid != null;
    }
    match /chats/{chatId} {
    		allow write, read: if request.auth.uid != null;
    }
  }
}
